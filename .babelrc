{
  "presets": ["@babel/env"],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    [
      "module-resolver", {
      "root": ["./src"],
      "alias": {
        "@helpers": "./src/helpers",
        "@middlewares": "./src/middlewares",
        "@utils": "./src/utils"
      }
    }]
  ]
}